package Modele;

import java.sql.Date;

/**
 *
 * @author martineau.cdi01
 */
public class Abonne {
    
    private int idAbonne;
    private Personne personne;
    private Boolean confirme;
    private Date dateFinAbonnement;
    
    public Abonne(int id, Personne pers, Boolean abonne, Date date){
        this.idAbonne = id;
        this.personne = pers;
        this.confirme = abonne;
        this.dateFinAbonnement = date;
    }

    public int getIdAbonne() {
        return idAbonne;
    }

    public void setIdAbonne(int idAbonne) {
        this.idAbonne = idAbonne;
    }

    public Personne getPersonne() {
        return personne;
    }

    public void setPersonne(Personne personne) {
        this.personne = personne;
    }

   public Boolean isConfirme() {
        return confirme;
    }

    public void setAbonne(Boolean abonne) {
        this.confirme = abonne;
    }

    public Date getDateFinAbonnement() {
        return dateFinAbonnement;
    }

    public void setDateFinAbonnement(Date dateAbonnement) {
        this.dateFinAbonnement = dateAbonnement;
    }
    
    
    
}
